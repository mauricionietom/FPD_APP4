    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExp</servlet-name>
        <url-pattern>/ServletExp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRegistro</servlet-name>
        <url-pattern>/ServletRegistro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMyperfil</servlet-name>
        <url-pattern>/ServletMyperfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletComentario</servlet-name>
        <url-pattern>/ServletComentario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
